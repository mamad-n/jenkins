node {

    docker.image('localhost:5000/alpine:3.18.4').inside{
  
		checkout scm

        stage('Build') {
        sh 'echo from build'
		sh 'echo you branch is -- $env.BRANCH_NAME'
		sh 'echo you branch is -- $BRANCH_NAME'
        sh 'sleep 100'
        }
        stage ('inner build'){
            sh 'echo from innner build : nemidonam'
        }

        stage ('inner test'){
            sh 'echo from inner test'
        }

    }
}

